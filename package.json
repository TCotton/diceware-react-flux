{
  "name": "diceware-react-flux",
  "version": "0.1.1",
  "description": "Diceware project built on top of React / Flux base.",
  "repository": "https://github.com/TCotton/diceware-react-flux",
  "license": "MIT",
  "main": "index.js",
  "config": {
    "buildDir": "./build",
    "buildDirTests": "./build_tests",
    "devHost": "localhost",
    "devPort": 8000
  },
  "scripts": {
    "build": "NODE_ENV=production bump --prompt && npm run webpack",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "env": "env",
    "lint": "eslint --ext .js --ext .jsx ./app ./webpack && echo No linting errors.",
    "prebuild": "npm run clean",
    "prestart": "npm install",
    "pretest": "npm install && npm run lint",
    "pretest-travis": "npm install && npm run lint",
    "start": "NODE_ENV=development npm run lint && node dev-server ./webpack/config",
    "test": "NODE_ENV=test karma start",
    "test-travis": "NODE_ENV=test karma start --single-run",
    "webpack": "webpack --colors --progress --config ./webpack/config"
  },
  "dependencies": {
    "body-parser": "^1.14.1",
    "classnames": "^2.1.3",
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.0",
    "express": "^4.13.3",
    "flux": "^2.0.3",
    "helmet": "^0.14.0",
    "helmet-csp": "^0.3.0",
    "method-override": "^2.3.5",
    "react": "^0.13.3",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "babel-core": "^5.8.3",
    "babel-eslint": "^3.1.23",
    "babel-loader": "^5.3.1",
    "babel-plugin-rewire": "^0.1.8",
    "babel-runtime": "^5.6.15",
    "chai": "^3.0.0",
    "css-loader": "^0.15.2",
    "css-mqpacker": "^4.0.0",
    "eslint": "^1.8.0",
    "eslint-plugin-react": "^3.6.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "glob": "^5.0.13",
    "html-loader": "^0.3.0",
    "jscs": "^2.5.0",
    "jscs-loader": "^0.2.0",
    "json-loader": "^0.5.2",
    "karma": "^0.13.2",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "0.1.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.2",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-sinon": "^1.0.4",
    "karma-source-map-support": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.5.1",
    "lodash": "^3.10.0",
    "mocha": "^2.2.5",
    "mocha-loader": "^0.7.1",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.4.1",
    "opn": "^3.0.2",
    "phantomjs": "^1.9.17",
    "postcss-fakeid": "^1.0.1",
    "postcss-flexbugs-fixes": "^2.0.0",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^0.6.0",
    "postcss-mq-keyframes": "^0.2.1",
    "postcss-short": "^1.3.1",
    "postcss-will-change": "^1.0.0",
    "react-hot-loader": "^1.2.8",
    "rimraf": "^2.4.1",
    "sass-lint": "^1.3.2",
    "sass-loader": "^3.1.1 ",
    "sinon": "^1.15.4",
    "source-map-support": "^0.3.2",
    "style-loader": "^0.12.3",
    "template-html-loader": "0.0.3",
    "version-bump-prompt": "^1.5.2",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.10.1"
  },
  "engines": {
    "node": ">=4.2.0"
  }
}